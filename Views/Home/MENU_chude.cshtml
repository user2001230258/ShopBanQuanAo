@using webmvc.Models
@model List<DANHMUC>


<div class="container mt-4">
  
    <div class="card">
        <div class="card-header bg-primary text-white text-center fw-bold fs-4 ">
            DANH MỤC SẢN PHẨM
        </div>
       
        <ul class="list-group list-group-flush">
            @foreach (var dm in Model)
            {
                <li class="list-group-item list-group-item-action fs-5">
                    <a href="#" class="text-decoration-none text-dark d-block">
                        @dm.TenDM
                    </a>
                </li>
            }
        </ul>
    </div>
</div>

